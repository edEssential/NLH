<section class="parallax-bg2 parallax colored clearfix marginUp" id="form-home" data-type="background" data-speed="10">
	<div class="content dark padded container">
		
		<div class="title grid-full">
			<h2 class="formTitle">Edit your contact information below!</h2>
		</div>

		<div class="cmsWrapper3">
				<div class="formWrapper">
					<%= form_for(@contact) do |f| %>
						<%= f.label :Contact_header, class: 'formText' %><br/>
			    		<%= f.text_area :title, class: 'email text_areaArea'  %><br /><br />
						<%= f.label :Contact_text, class: 'formText' %><br/>
			    		<%= f.text_area :text, class: 'email text_areaArea'  %><br /><br />
						<%= f.label :Thanks_title, class: 'formText' %><br/>
			    		<%= f.text_area :thankstitle, class: 'email text_areaArea' %><br /><br />
						<%= f.label :Thanks_text, class: 'formText' %><br/>
			    		<%= f.text_area :thankstext, class: 'email text_areaArea' %><br /><br />
			    		<%= f.submit %>
					<% end %>
				</div>
			</div>
		</div>
	</div>
</section>